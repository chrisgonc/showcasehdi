FUNCTION F_MASKING_MEASURE(input DOUBLE) RETURNS output DOUBLE LANGUAGE SQLSCRIPT SQL SECURITY DEFINER
AS BEGIN
   --No data exception would return full data
   DECLARE EXIT HANDLER FOR SQLEXCEPTION SELECT 0 into output FROM DUMMY;
   select am_mask_value_1 into output from "AUTHORIZATION_MAPPING" WHERE AM_USER_ID = SESSION_USER;
END;